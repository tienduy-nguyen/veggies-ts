{
    "name": "veggies-ts",
    "version": "0.0.16",
    "description": "Veggies is an awesome cucumber-js library for API/CLI testing. Great for testing APIs built upon Express, Koa, HAPI and others.",
    "tags": [
        "bdd",
        "cucumber",
        "gherkin",
        "testing",
        "api",
        "http",
        "cli"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tienduy-nguyen/veggies-ts.git"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "bin": {
        "veggies": "./bin/veggies.js"
    },
    "files": [
        "lib/**/*",
        "bin/**/*",
        "*.d.ts"
    ],
    "author": "plouc <https://github.com/plouc>",
    "maintainers": [
        {
            "name": "Tien Duy NGUYEN"
        }
    ],
    "license": "MIT",
    "engines": {
        "node": ">=14"
    },
    "engineStrict": true,
    "dependencies": {
        "@cucumber/cucumber": "8.2.1",
        "arg": "5.0.1",
        "chai": "4.3.6",
        "dayjs": "1.11.2",
        "fast-glob": "3.2.11",
        "fs-extra": "10.1.0",
        "jest-diff": "28.1.0",
        "js-yaml": "4.1.0",
        "lodash": "4.17.21",
        "natural-compare": "1.4.0",
        "pretty-format": "28.1.0",
        "request": "2.88.2",
        "tough-cookie": "4.0.0"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^17.0.0",
        "@swc/core": "^1.2.186",
        "@swc/jest": "^0.2.21",
        "@types/chai": "4.3.1",
        "@types/fs-extra": "9.0.13",
        "@types/jest": "^27.5.1",
        "@types/js-yaml": "4.0.5",
        "@types/lodash": "4.14.182",
        "@types/mustache": "^4.1.2",
        "@types/natural-compare": "^1.4.1",
        "@types/node": "^17.0.34",
        "@types/request": "2.48.8",
        "@types/sinon": "^10.0.11",
        "@types/tough-cookie": "4.0.2",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "chalk": "^4.1.2",
        "commitlint": "^17.0.0",
        "conventional-changelog-cli": "^2.2.2",
        "coveralls": "^3.1.1",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "8.0.1",
        "jest": "^28.1.0",
        "jsdoc": "^3.6.10",
        "lint-staged": "^12.4.1",
        "minami": "^1.2.3",
        "nock": "^13.2.4",
        "prettier": "^2.6.2",
        "sinon": "^14.0.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4"
    },
    "scripts": {
        "build": "tsc --build tsconfig.lib.json && cp src/index.js lib/index.js  && cp src/index.d.ts lib/index.d.ts && cp -R src/world lib/world",
        "prepublishOnly": "rm -rf lib && yarn build",
        "test": "jest --verbose --colors tests",
        "test-cover": "jest --verbose --colors --coverage",
        "test-cli": "veggies --require tests/functional/support tests/functional/features",
        "coverage": "cat ./coverage/lcov.info | coveralls",
        "fmt": "prettier --color --loglevel warn --write \"{src,tests,scripts}/**/*.js\" \"*.{yml,yaml,json}\"",
        "check-fmt": "prettier --list-different \"{src,tests,scripts}/**/*.js\" \"*.{yml,yaml,json}\"",
        "lint": "eslint . --quiet --cache",
        "lint-fix": "eslint --fix . --quiet --cache",
        "lint-local": "yarn lint && yarn fmt",
        "doc": "jsdoc -c .jsdoc.json --verbose",
        "doc-pub": "yarn doc && gh-pages -d _doc",
        "examples": "veggies --require 'examples/cucumber.js' --require 'examples/support/**/*.[tj]s' 'examples/features'",
        "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s"
    },
    "packageManager": "yarn@3.2.0"
}
